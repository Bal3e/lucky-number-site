<%- include('partials/header.ejs') %>

<section>
    <div class="table-container">
        <table id="dataTable" class="display">
            <thead>
                <tr>
                    <th>No</th>
                    <th>Data</th>
                </tr>
            </thead>
            <tbody>
                <% locals.dataSites.forEach((item, index) => { %> 
                    <tr>
                        <td><%= index + 1 %></td>
                        <td><%= item %></td>
                    </tr>    
                <% }) %>
            </tbody>
        </table>
    </div>
</section>

<%- include('partials/footer.ejs') %>
